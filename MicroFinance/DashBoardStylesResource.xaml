<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MicroFinance">

    <!--Main Display Text style-->
    <Style x:Key="DashboardBtnText" TargetType="{x:Type TextBlock}">
        <Setter Property="FontSize" Value="16"></Setter>
        <Setter Property="FontWeight" Value="SemiBold"></Setter>
        <Setter Property="TextWrapping" Value="Wrap"></Setter>
        <Setter Property="TextAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
    </Style>

    <!--Notification batch Text style-->
    <Style x:Key="NotifyText" TargetType="{x:Type TextBlock}">
        <Setter Property="FontSize" Value="16"></Setter>
        
        <Setter Property="FontWeight" Value="Bold"></Setter>
        <Setter Property="Foreground" Value="White"></Setter>
        <Setter Property="TextAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
    </Style>

    <!--Notification Batch Border-->
    <Style x:Key="NotifyCount" TargetType="{x:Type Border}">
        <Setter Property="CornerRadius" Value="50"></Setter>
        <Setter Property="BorderBrush" Value="White"></Setter>
        <Setter Property="BorderThickness"  Value="3"/>
        <Setter Property="Background" Value="Red"></Setter>
        <Setter Property="HorizontalAlignment" Value="Right"></Setter>
        <Setter Property="VerticalAlignment" Value="Top"></Setter>
    </Style>
    <Style x:Key="NotifyCountBig" TargetType="{x:Type Border}">
        <Setter Property="CornerRadius" Value="15"></Setter>
        <Setter Property="BorderBrush" Value="White"></Setter>
        <Setter Property="BorderThickness"  Value="3"/>
        <Setter Property="Background" Value="Red"></Setter>
        <Setter Property="HorizontalAlignment" Value="Right"></Setter>
        <Setter Property="VerticalAlignment" Value="Top"></Setter>
    </Style>

    <!--Dashboard Buttons main Border-->
    <Style x:Key="MainBorder" TargetType="{x:Type Border}">
        <Setter Property="Border.Background" Value="#FEF8F1"></Setter>
        <Setter Property="Border.CornerRadius" Value="10"></Setter>
        <Style.Triggers>
            <Trigger Property="Border.IsMouseOver" Value="True">
                <Setter Property="Border.Background" Value="LightBlue"></Setter>
            </Trigger>
        </Style.Triggers>
    </Style>


    <!--Add Customer Template-->
    <Style TargetType="{x:Type Button}" x:Key="NavigateAddCustomer">
        <Style.Resources>
            <Style TargetType="{x:Type Border}">
                <Setter Property="CornerRadius" Value="10"></Setter>
            </Style>
        </Style.Resources>
        <Setter Property="Background" Value="#FEF8F1"></Setter>
        <Setter Property="BorderThickness" Value="0"></Setter>
        <Setter Property="Content">
            <Setter.Value>
                <ContentPresenter>
                    <ContentPresenter.ContentTemplate>
                        <DataTemplate>
                            <Grid Height="80" Width="250">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="5*"></RowDefinition>
                                    <RowDefinition Height="90*"></RowDefinition>
                                    <RowDefinition Height="5*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5*"/>
                                    <ColumnDefinition Width="25*"/>
                                    <ColumnDefinition Width="65*"/>
                                    <ColumnDefinition Width="5*"/>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="1" Grid.Row="1" Width="35" Height="35" Source=".\Asserts\Icons\addCustomer.png" HorizontalAlignment="Center" VerticalAlignment="Center"></Image>
                                <TextBlock Grid.Column="2" Grid.Row="1" Text="Add Customer" HorizontalAlignment="Left" Margin="20,0,0,0" Style="{StaticResource DashboardBtnText}"></TextBlock>
                            </Grid>
                        </DataTemplate>
                    </ContentPresenter.ContentTemplate>
                </ContentPresenter>
            </Setter.Value>
        </Setter>
    </Style>

